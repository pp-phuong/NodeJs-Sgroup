<div class="section section-post-block tw-border-t tw-border-soft">
    <div class="tw-container">
        <div class="section-header tw-text-center tw-mb-8">
            <h2 class="section-title tw-text-3xl tw-mb-0"> Categories </h2>
            <p class="section-subtitle tw-text-soft tw-mt-2 tw-mb-0">All of posts</p>
        </div>
        <div class="footer-nav tw-mb-6 tw-hidden md_tw-block">
            <ul id="menu-footer" class="menu tw-flex tw-justify-center ">
                <% categories.forEach(category => { %>
                <div id="menu-item-96" class="menu tw-flex tw-justify-center ">
                    <a href="/category/<%= category.category_slug %>"><button
                            class="btn btn-dark"><%= category.category_name %></button>
                    </a>
                </div>
                <% }); %>
            </ul>
        </div>
    </div>
</div>